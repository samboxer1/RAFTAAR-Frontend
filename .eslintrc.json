// .eslintrc.json
{
  "extends": ["react-app", "plugin:jsx-a11y/recommended"],
  "plugins": ["jsx-a11y"],
  "rules": {
    "react/jsx-key": "error",
    "jsx-a11y/anchor-is-valid": "error",
    "jsx-a11y/alt-text": ["warn", {
      "elements": ["img"],
      "img": ["Image"]
    }],
    "react/prop-types": "warn"
  }
}